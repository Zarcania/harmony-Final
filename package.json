{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173 --strictPort",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:rpc": "node ./scripts/test_available_slots.mjs",
    "supabase:export": "powershell -ExecutionPolicy Bypass -File ./scripts/supabase_export.ps1",
    "supabase:types": "supabase gen types typescript --project-id lmpfrrkqdevxkgimvnfw --schema public > src/types/database.types.ts",
    "supabase:csv-export": "powershell -ExecutionPolicy Bypass -File ./scripts/export_csv_from_supabase.ps1",
    "supabase:align": "npm run supabase:types && npm run supabase:csv-export"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^2.1.4"
  },
  "vitest": {
    "environment": "jsdom",
    "setupFiles": [
      "./src/setupTests.ts"
    ]
  }
}
